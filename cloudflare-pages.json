{
  "name": "portfolio",
  "build": {
    "command": "npm run build",
    "output_directory": ".next",
    "root_directory": "/"
  },
  "routes": [
    { "pattern": "/api/send", "script": "src/app/api/send/route.ts" },
    { "pattern": "/*", "script": null }
  ],
  "env": {
    "RESEND_API_KEY": "re_dummy_key_for_build_only"
  }
} 